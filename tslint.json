{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "object-literal-sort-keys": false,
        "interface-name": false,
        "member-access": false,
        "ordered-imports": false,
        "no-trailing-whitespace": false,
        "variable-name": {
            "options": [
                "allow-snake-case"
            ]
        }
    },
    "rulesDirectory": []
}